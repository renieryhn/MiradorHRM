            LoadTabData($('#custom-tabs-two-Empleado-tab')[0], 'loading-spinner-Empleado');
        }
    });

    function getParameterByName(name, url = window.location.href) {
        name = name.replace(/[\[\]]/g, '\\$&');
        var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, ' '));
    }

   

    function LoadTabData(obj, spinnerId) {
        var tab = obj.id;
        var sfuncion = "";
       

        $('#' + spinnerId).show();

        if (tab === 'custom-tabs-two-profile-tab') {
            sfuncion = "LoadContactos";
        } else if (tab === 'custom-tabs-two-messages-tab') {
            sfuncion = "LoadContratos";
        } else if (tab === 'custom-tabs-two-settings-tab') {
            sfuncion = "LoadEducacion";          
        } else if (tab === 'custom-tabs-two-Experiencium-tab') {
            sfuncion = "LoadExperiencia";
        } else if (tab === 'custom-tabs-two-Habilidad-tab') {
            sfuncion = "LoadHabilidad";
        } else if (tab === 'custom-tabs-two-Ausencias-tab') {
            sfuncion = "LoadAusencias";
        } else if (tab === 'custom-tabs-two-Empleado-tab') {
            sfuncion = "LoadEmpleado";
        }

        var empleadoId = '@Model.IdEmpleado';
        var surl = '/Empleado/' + sfuncion + '?id=' + empleadoId;

        $.ajax({
            url: surl,
            method: 'GET',
            success: function (data) {
                if (tab === 'custom-tabs-two-profile-tab') {
                    $('#custom-tabs-two-profile').html(data);
                }
                else if (tab === 'custom-tabs-two-messages-tab') {
                    $('#custom-tabs-two-messages').html(data);
                }
                else if (tab === 'custom-tabs-two-settings-tab') {
                    $('#custom-tabs-two-settings').html(data);
                }
                else if (tab === 'custom-tabs-two-Experiencium-tab') {
                    $('#custom-tabs-two-Experiencium').html(data);
                }
                else if (tab === 'custom-tabs-two-Habilidad-tab') {
                    $('#custom-tabs-two-Habilidad').html(data);
                }
                else if (tab === 'custom-tabs-two-Ausencias-tab') {
                    $('#custom-tabs-two-Ausencias').html(data);
                }
                else if (tab === 'custom-tabs-two-Empleado-tab') {
                    $('#custom-tabs-two-Empleado').html(data);
                }

                $('#' + spinnerId).hide();
            },
            error: function (xhr, status, error) {
                // Maneja el error si la solicitud AJAX falla
                console.error(error);

                $('#' + spinnerId).hide();
            }
            
        });
    }

