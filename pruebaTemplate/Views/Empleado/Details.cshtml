        var tab = getParameterByName('tab');
       
        if (tab) {         
            var spinnerId = 'loading-spinner-' + tab.replace('-tab', '');
            LoadTabData($(tab)[0], spinnerId); // Se pasa el ID completo del spinner
            storeActiveTab(tab + '-tab'); // Almacena la pestaña activa
        } else {
            // Verificar si hay una pestaña activa almacenada en sessionStorage
            var activeTab = sessionStorage.getItem('activeTab');
            if (activeTab) {
                $('#' + activeTab).tab('show');
                var activeTabName = activeTab ? activeTab.slice(16, -4) : null;
                console.log(activeTabName)
                var spinnerId = 'loading-spinner-' + activeTabName;
                console.log(spinnerId)
                LoadTabData($('#' + activeTab)[0], spinnerId); // Se pasa el ID completo del spinner
            }
        }

        // Almacenar la pestaña activa antes de recargar la página
        $('.nav-link').on('click', function () {
            var tabId = $(this).attr('id');
            storeActiveTab(tabId);
        });
    });





</script>




