                                </div>
                                <div class="col-md-8">
                                    <input  type="text" class="form-control" disabled value="@Html.DisplayFor(model => model.ComentarioAprobacion)"/>
                                </div>
                       </div> 
                        <div class="form-group row">
                                <div class="col-md-2">
                                    <label class="control-label">@Html.DisplayNameFor(model => model.Activo)</label>
                                </div>
                                <div class="col-md-8">
