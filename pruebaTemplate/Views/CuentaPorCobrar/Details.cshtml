                                    <label class="control-label">@Html.DisplayNameFor(model => model.FechaModificacion)</label>
                                </div>
                                <div class="col-md-8">
                                    <input  type="text" class="form-control" disabled value="@Html.DisplayFor(model => model.FechaModificacion)"/>
                                </div>
                       </div> 
                        <div class="form-group row">
                                <div class="col-md-2">
                                    <label class="control-label">@Html.DisplayNameFor(model => model.CreadoPor)</label>
                                </div>
                                <div class="col-md-8">
                                    <input  type="text" class="form-control" disabled value="@Html.DisplayFor(model => model.CreadoPor)"/>
                                </div>
                       </div> 
                        <div class="form-group row">
                                <div class="col-md-2">
                                    <label class="control-label">@Html.DisplayNameFor(model => model.ModificadoPor)</label>
                                </div>
                                <div class="col-md-8">
                                    <input  type="text" class="form-control" disabled value="@Html.DisplayFor(model => model.ModificadoPor)"/>
                                </div>
                       </div> 
                        <div class="form-group row">
                       </div> 
                        <div class="form-group row">
                            <div class="col-md-2">
                                 <label class="control-label">@Html.DisplayNameFor(model => model.IdDeduccionNavigation)</label>
                             </div>
                             <div class="col-md-8">
                                 <input type="text" class="form-control" value="@Html.DisplayFor(model => model.IdDeduccionNavigation.NombreDeduccion)" disabled/>
                             </div>
                       </div> 
                        <div class="form-group row">
                            <div class="col-md-2">
                                 <label class="control-label">@Html.DisplayNameFor(model => model.IdEmpleadoNavigation)</label>
                             </div>
                             <div class="col-md-8">
                                 <input type="text" class="form-control" value="@Html.DisplayFor(model => model.IdEmpleadoNavigation.NombreCompleto)" disabled/>
                             </div>
                       </div> 
                </div>
            </div>
        </div>
         <div class="card-footer">
             <a asp-action="Edit" asp-route-id="@Model.IdCuentaPorCobrar" type="submit" class="btn btn-primary" style="background-color: var(--blue);">Editar</a>
             <a asp-action="Index" class="btn btn-secondary">Regresar a la Lista</a>
          </div>
    </div>
</div>

